        <TextField fx:id="password" layoutX="20.0" layoutY="240.0" prefHeight="30.0" prefWidth="280.0" />
        <Text fill="#203169" layoutX="20.0" layoutY="290.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Georgia; -fx-font-weight: Bold;" text="Access level">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Text>
        <ChoiceBox fx:id="accessLevel" layoutX="20.0" layoutY="300.0" prefWidth="60.0" style="-fx-background-color: #00000000;" />
        <Button fx:id="signUp" layoutX="20.0" layoutY="360.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="280.0" style="-fx-background-color: #203169; -fx-text-fill: #FFFFFF; -fx-font-weight: bold;" text="Sign up">
