<AnchorPane prefHeight="524.0" prefWidth="320.0" style="-fx-background-color: #FFFFFF; -fx-background-radius: 10; -fx-border-radius: 10;" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LibraryManagement.controllers.signUp">
    <children>
        <Button fx:id="signIn1" layoutX="23.0" layoutY="40.0" mnemonicParsing="false" onAction="#showSignIn" opacity="0.5" style="-fx-background-color: #FFFFFF; -fx-text-fill: #203169; -fx-font-family: Georgia; -fx-effect: NULL;" text="Sign in">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Button>
        <Label layoutX="150.0" layoutY="48.0" style="-fx-font-family: Georgia;" text="Sign up" textFill="#203169">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Text fill="#203169" layoutX="20.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Georgia; -fx-font-weight: Bold;" text="Name">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Text>
        <TextField fx:id="name" layoutX="20.0" layoutY="120.0" prefHeight="30.0" prefWidth="280.0" />
        <Text fill="#203169" layoutX="20.0" layoutY="170.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-family: Georgia; -fx-font-weight: Bold;" text="Phone number">
